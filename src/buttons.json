{
  "name": "Architecture",
  "general": {
    "fontFamily": "'Montserrat', sans-serif",
    "borderWidth": "1px",
    "textTransform": "uppercase"
  },
  "size": {
    "sm": {
      "padding": "15px 17px",
      "borderRadius": 0,
      "fontSize": "12px"
    },
    "md": {
      "padding": "19px 21px",
      "borderRadius": 0,
      "fontSize": "12px"
    },
    "lg": {
      "padding": "24px 31px",
      "borderRadius": 0,
      "fontSize": "12px"
    }
  },
  "settings": {
    "primary": {
      "default": {
        "bg": {
          "color": ["brand"]
        },
        "color": ["lightShade"],
        "border": {
          "color": ["transparent"]
        }
      },
      "hover": {
        "bg": {
          "color": ["lightAccent"]
        },
        "color": ["lightShade"]
      },
      "active": {
        "bg": {
          "color": ["brand"]
        },
        "color": ["lightShade"]
      }
    },
    "primaryAlt": {
      "default": {
        "bg": {
          "color": ["lightAccent"]
        },
        "color": ["lightShade"],
        "border": {
          "color": ["transparent"]
        }
      },
      "hover": {
        "bg": {
          "color": ["brand"]
        },
        "color": ["lightShade"]
      },
      "active": {
        "bg": {
          "color": ["lightAccent"]
        },
        "color": ["lightShade"]
      }
    },
    "secondary": {
      "default": {
        "bg": {
          "color": ["transparent"]
        },
        "color": ["darkShade?lighten=2.35", "lightShade"],
        "border": {
          "color": ["lightShade?darken=19.61", "darkShade?lighten=35.69"]
        }
      },
      "hover": {
        "bg": {
          "color": ["lightShade?darken=7.84", "lightShade"]
        },
        "color": ["darkShade?lighten=2.35", "darkShade"]
      },
      "active": {
        "bg": {
          "color": ["transparent"]
        }
      }
    },
    "secondaryAlt": {
      "default": {
        "bg": {
          "color": ["lightShade?darken=7.84", "lightShade"]
        },
        "color": ["darkShade?lighten=2.35", "darkShade?lighten=2.35"],
        "border": {
          "color": ["lightShade?darken=19.61", "darkShade?lighten=35.69"]
        }
      },
      "hover": {
        "bg": {
          "color": ["transparent"]
        }
      },
      "active": {
        "bg": {
          "color": ["lightShade?darken=7.84", "lightShade"]
        }
      }
    }
  }
}
