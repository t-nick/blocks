{
  "name": "Klink",
  "general": {
    "fontFamily": "'Open Sans', sans-serif",
    "borderWidth": "2px",
    "textTransform": "uppercase",
    "letterSpacing": "0.3px"
  },
  "size": {
    "sm": {
      "padding": "13.5px 40px",
      "borderRadius": "23px",
      "fontSize": "11px"
    },
    "md": {
      "padding": "14px 49px",
      "borderRadius": "25px",
      "fontSize": "13px"
    },
    "lg": {
      "padding": "16px 69px",
      "borderRadius": "27px",
      "fontSize": "13px"
    }
  },
  "settings": {
    "primary": {
      "default": {
        "fontWeight": 600,
        "bg": {
          "color": ["brand"],
          "gradient": {
            "angle": "90deg",
            "colors": [
              [
                "brand",
                "darkAccent"
              ]
            ]
          }
        },
        "color": ["lightShade"],
        "border": {
          "color": ["transparent"]
        },
        "shadow": {
          "x": 0,
          "y": "10px",
          "blur": "23.8px",
          "spread": "1.2px",
          "color": [
            "brand?setAlpha=0.3",
            "brand?setAlpha=0.5"
          ]
        }
      },
      "hover": {
        "bg": {
          "color": ["brand"],
          "gradient": {
            "angle": "90deg",
            "colors": [
              [
                "brand?darken=10.98&desaturate=16.33",
                "darkAccent?darken=12.35&desaturate=26.38"
              ]
            ]
          }
        }
      }
    },
    "primaryAlt": {
      "default": {
        "fontWeight": 600,
        "bg": {
          "color": ["brand", "lightShade"]
        },
        "color": ["lightShade", "darkShade?lighten=28.24&desaturate=2.6&spin=355"],
        "border": {
          "color": ["transparent"]
        }
      },
      "hover": {
        "bg": {
          "color": ["darkAccent"]
        },
        "color": [
          "lightShade", "brand"
        ]
      }
    },
    "secondary": {
      "default": {
        "fontWeight": 600,
        "bg": {
          "color": ["transparent"]
        },
        "color": ["darkShade?lighten=28.24&desaturate=2.6&spin=355", "lightShade"],
        "border": {
          "color": ["brand"]
        }
      },
      "hover": {
        "border": {
          "color": ["brand?darken=10.98&desaturate=16.33"]
        }
      }
    },
    "secondaryAlt": {
      "default": {
        "fontWeight": 600,
        "bg": {
          "color": ["transparent"]
        },
        "color": ["darkShade?lighten=28.24&desaturate=2.6&spin=355", "lightShade"],
        "border": {
          "color": ["brand", "lightShade"]
        }
      },
      "hover": {
        "color": ["lightShade", "darkShade"],
        "border": {
          "color": ["brand?darken=10.98&desaturate=16.33", "transparent"]
        }
      }
    }
  }
}