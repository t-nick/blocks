{
  "name": "Rattata",
  "general": {
    "fontFamily": "'Rubik', sans-serif",
    "borderWidth": "2px",
    "textTransform": "uppercase",
    "letterSpacing": "1.6px"
  },
  "size": {
    "sm": {
      "padding": "8.5px 42px",
      "borderRadius": 0,
      "fontSize": "11px"
    },
    "md": {
      "padding": "10.5px 42.5px",
      "borderRadius": 0,
      "fontSize": "13px"    
    },
    "lg": {
      "padding": "20.5px 50.5px",
      "borderRadius": 0,
      "fontSize": "14px"
    }
  },
  "settings": {
    "primary": {
      "default": {
        "bg": {
          "color": ["brand"]
        },
        "color": ["lightShade"],
        "border": {
          "color": ["transparent"]
        }
      },
      "hover": {
        "bg": {
          "color": ["brand?lighten=4.12&saturate=17.16"]
        },
        "color": []
      },
      "active": {
        "color": ["brand"],
        "bg": {
          "color": ["darkShade"]
        },
        "border": {
          "color": ["brand"]
        }
      }
    },
    "primaryAlt": {
      "default": {
        "bg": {
          "color": ["brand"]
        },
        "color": ["lightShade"],
        "border": {
          "color": ["transparent"]
        }
      },
      "hover": {
        "bg": {
          "color": ["brand?lighten=4.12&saturate=17.16"]
        }
      },
      "active": {
        "color": ["brand"],
        "bg": {
          "color": ["darkShade"]
        },
        "border": {
          "color": ["brand"]
        }
      }
    },
    "secondary": {
      "default": {
        "bg": {
          "color": ["transparent"]
        },
        "color": ["darkShade", "lightShade"],
        "border": {
          "color": ["lightShade?darken=4.71&saturate=16.67&spin=225", "lightShade?darken=13.92&saturate=4.23&spin=220"]
        }
      },
      "hover": {
        "color": ["brand", "brand"]
      },
      "active": {
        "bg": {
          "color": ["lightShade?darken=4.71&saturate=16.67&spin=225", "lightShade?darken=13.92&saturate=4.23&spin=220"]
        },
        "color": ["darkShade", "lightShade"],
        "border": {
          "color": ["transparent", "transparent"]
        }
      }
    },
    "secondaryAlt": {
      "default": {
        "bg": {
          "color": ["transparent"]
        },
        "color": ["darkShade", "lightShade"],
        "border": {
          "color": ["lightShade?darken=4.71&saturate=16.67&spin=225", "lightShade?darken=13.92&saturate=4.23&spin=220"]
        }
      },
      "hover": {
        "color": ["brand", "brand"]
      },
      "active": {
        "bg": {
          "color": ["lightShade?darken=4.71&saturate=16.67&spin=225", "lightShade?darken=13.92&saturate=4.23&spin=220"]
        },
        "color": ["darkShade", "lightShade"],
        "border": {
          "color": ["transparent", "transparent"]
        }
      }
    }
  }
}
